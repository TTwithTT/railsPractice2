<%= @reservations.each do |reservation| %>
	<%= image_tag reservation.room.avatar.url %>
	<%= link_to reservation.room.name, reservation.room %>
	<%= reservation.room.description %>
	<%= reservation.totalAmoutOfPrice %>
	<%= reservation.fromDate %>
	<%= reservation.toDate %>
	<%= reservation.created_at %>
	<%= link_to '編集', [:edit, reservation] %>
	<%= link_to '削除', reservation, method: :delete, data: { confirm: '削除しますか？'} %>
<% end %>
