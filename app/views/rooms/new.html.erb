<%= form_with model: @room do |f| %>
	<%= f.label :name ,'物件名' %>
	<%= f.text_field :name %>
	<%= f.label :description ,'概要' %>
	<%= f.text_area :description %>
	<%= f.label :price ,'料金' %>
	<%= f.number_field :price %>
	<%= f.label :address ,'住所' %>
	<%= f.text_area :address %>
	<%= f.label :image ,'画像' %>
	<%= f.file_field :image %>
	<%= f.submit '物件を登録する' %>
<% end %>
